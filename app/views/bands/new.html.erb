<h1>New Band</h1>

<%= link_to "Back to All Bands", bands_url %>

<form action="<%= bands_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label for="band_name">Band Name:</label>
  <input type="text" name="band[name]">
  <br>

  <input type="Submit">
</form>
